<?php
// src/Bookshop\BookshopBundle\Entity/User.php

namespace Bookshop\BookshopBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="fos_user")
 */
class User extends BaseUser
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    
    /**
     * @var string
     *
     * @ORM\Column(name="first_name", type="string", length=255)
     */
    protected $first_name;
    
    /**
     * @var string
     *
     * @ORM\Column(name="last_name", type="string", length=255)
     */
    protected $last_name;
    
    /**
     * @var string
     *
     * @ORM\Column(name="mobile", type="string", length=20)
     */
    protected $mobile;
    
    /**
     * @var string
     *
     * @ORM\Column(name="gender", type="string", length=1)
     */
    protected $gender;
    
    /**
     * @var string
     *
     * @ORM\Column(name="education", type="string", length=1)
     */
    protected $education;
    
    /**
     * @var string
     *
     * @ORM\Column(name="billing_address_id", type="integer")
     */
    protected $billing_address_id;
    
    /**
     * @var string
     *
     * @ORM\Column(name="shipping_address_id", type="integer")
     */
    protected $shipping_address_id;

    public function __construct()
    {
        parent::__construct();
        // your own logic
    }
}